#include <bits/stdc++.h>
#include <cstring>
#include <string>
using namespace std;
#define PB push_back
#define MP make_pair
#define F(i,a,b) for(int i = a ; i < b; i++)

int main() {
  string line;
  bool flag = true;

  while(getline(cin,line)){
    F(i, 0, line.length()){
      if(line[i] == '"'){
        if(flag){
          cout << "``";
        }else{
          cout << "''";
        }
        flag = !flag;
      }else{
        cout << line[i];
      }
    }
    cout << "\n";
  }
}


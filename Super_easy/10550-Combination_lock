#include <bits/stdc++.h>
#include <cstring>
#include <string>
using namespace std;
#define PB push_back
#define MP make_pair
#define F(i,a,b) for(int i = a ; i < b; i++)

int main() {
  int s,a,b,c;
  while (cin >> s >> a >> b >> c && ( s || a || b || c )) {
    int total = 720;
    total += ((40 + s - a)% 40)*9;
    total += 360;
    total += ((40+b-a)%40)*9;
    total += ((40+b-c)%40)*9;
    cout << total << endl;
  }
} 

